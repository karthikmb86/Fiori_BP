{
	"_version": "1.12.0",
	"sap.app": {
		"id": "ns.HTML5Module",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		},
		"dataSources": {
			"mainService": {
				"uri": "S4DeloitteDemo/sap/opu/odata/sap/YY1_SALESHISTORYOLD_CDS/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata2.xml"
				}
			},
			"BusinessPartnerService": {
				"uri": "S4DeloitteDemo/sap/opu/odata/sap/API_BUSINESS_PARTNER/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata2.xml"
				}
			},
			"employeeRemote": {
				"uri": "/here/goes/your/serviceUrl/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "ns.HTML5Module.view.App",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.core": {
					
				},
				"sap.m": {
					
				},
				"sap.ui.layout": {
					
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ns.HTML5Module.i18n.i18n"
				}
			},
			"": {
				"dataSource": "BusinessPartnerService",
				"preload": true,
				"settings": {
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline",
					"refreshAfterChange": false
				}
			}

		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "ns.HTML5Module.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"clearControlAggregation": false,
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "appHome",
					"target": "home"
				},
				{
					"pattern": "employees",
					"name": "employeeList",
					"target": "employees"
				},
				{
					"pattern": "A_BusinessPartner/{employeeId}",
					"name": "employee",
					"target": "employee"
				},
				{
					"pattern": "student",
					"name": "student",
					"target": "student"
				}
			],
			"targets": {
				"home": {
					"viewName": "Home",
					"viewLevel": 1
				},
				"notFound": {
					"viewName": "NotFound",
					"transition": "show"
				},
				"employees": {
					"viewPath": "ns.HTML5Module.view.employees",
					"viewName": "EmployeeList",
					"viewLevel": 2
				},
				"employee": {
					"viewName": "employees.Employee",
					"viewLevel": 3
				},
				"student": {
					"viewName": "Student",
					"viewLevel": 2
				}
			}
		}
	}
}
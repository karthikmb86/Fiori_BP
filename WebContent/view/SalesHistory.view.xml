<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ns.HTML5Module.controller.SalesHistory" displayBlock="true">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
					    <List noDataText="Drop list items here" items="{/A_BusinessPartner}" id="list0">
					        <items>
					            <StandardListItem press="getBPDetails" type="Navigation" title="{BusinessPartnerFullName}" icon="sap-icon://picture" id="item0" description="{Supplier}"/>
					        </items>
					    </List>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>